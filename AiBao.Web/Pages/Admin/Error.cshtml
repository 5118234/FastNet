@page
@model AiBao.Web.Pages.Admin.ErrorModel
@{
    ViewData["Title"] = "系统错误";
}

<section class="content-header">
    <h1>
        系统错误
    </h1>
    <ol class="breadcrumb">
        <li><a href="/admin/home"><i class="fa fa-dashboard"></i> 主界面</a></li>
        <li class="active">系统错误</li>
    </ol>
</section>
<section class="content">

    <div class="box box-danger">
        <div class="box-body">

            <h1 class="text-danger">错误提醒.</h1>
            <h2 class="text-danger">系统发生了错误，我们已经以邮件的形式通知了开发人员，请耐心等待处理！</h2>
            @if (Model.ShowRequestId)
            {
                <p>
                    <strong>Request ID:</strong> <code>@Model.RequestId</code>
                </p>
            }
        </div>
    </div>

</section>
